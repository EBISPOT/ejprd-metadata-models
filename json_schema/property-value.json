{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "additionalProperties": true,
    "required": [
        "PropertyValue"
    ],
    "properties": {
        "PropertyValue": {
            "$id": "#/properties/PropertyValue",
            "type": "object",
            "title": "The Propertyvalue Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "value": "value",
                    "name": "name",
                    "code": "code"
                }
            ],
            "additionalProperties": true,
            "required": [
                "name",
                "value",
                "code"
            ],
            "properties": {
                "name": {
                    "$id": "#/properties/PropertyValue/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "name"
                    ]
                },
                "value": {
                    "$id": "#/properties/PropertyValue/properties/value",
                    "type": "string",
                    "title": "The Value Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "value"
                    ]
                },
                "code": {
                    "$id": "#/properties/PropertyValue/properties/code",
                    "type": "string",
                    "title": "The Code Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "code"
                    ]
                }
            }
        }
    }
}
