{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "additionalProperties": true,
    "required": [
        "codingSystem"
    ],
    "properties": {
        "codingSystem": {
            "type": "object",
            "title": "The Codingsystem Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "label": "label",
                    "id": "id",
                    "url": "url",
                    "description": "description"
                }
            ],
            "additionalProperties": true,
            "required": [
                "id",
                "url",
                "label",
                "description"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "title": "The Id Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "id"
                    ]
                },
                "url": {
                    "type": "string",
                    "title": "The Url Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "url"
                    ]
                },
                "label": {
                    "type": "string",
                    "title": "The Label Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "label"
                    ]
                },
                "description": {
                    "type": "string",
                    "title": "The Description Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "description"
                    ]
                }
            }
        }
    }
}
