PREFIX sio: <http://semanticscience.org/resource/>


ejp:CatalogShape {
    dc:identifier  xsd:string ;
    dcterms:alternativename xsd:string ;
    sio:name xsd:string ;
    sio:description xsd:string ;
    ejprd:about @ejprd:codeShape ;
    obo:location @obo:organisationShape
    }

ejprd:codeShape{
    ejprd:id xsd:string ;
    sio:url xsd:string ;
    rdfs:label xsd:string ;
    ejprd:descritpiton xsd:string
    }

obo:organisationShape {
    obo:name xsd:string ;
    obo:facility xsd:string ;
    obo:department xsd:string ;
    sio:address @obo:locationShape
    }

obo:locationShape {
    sio:street xsd:string;
    sio:city xsd:string ;
    obo:code xsd:string ;
    sio:country xsd:string
    }
