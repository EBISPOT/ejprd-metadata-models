ejprd:codingsystem {
      ejprd:id  xsd:string ;
      sio:url xsd:string ;
      rdfs:label xsd:string ;
      ejprd:descritpiton @obo:BioBankShape
}


obo:BioBankShape {
  dc:identifier IRI /^http:\/\/purl\.org\/dc\/elements\/1.1\/id#[0-9]+/ ;
  ejprd:hostejpituition xsd:string;
  sio:name xsd:string ;
  sio:city xsd:string ;
  sio:country xsd:string
      }



